{
	"applications": {
		"gecko": {
			"id": "Send2Ged@secu.lu",
			"strict_min_version": "68.0"
		}
	},
	"manifest_version": 2,
	"name": "Send2Ged",
	"version": "1.0",
	"author": "benoit.noss@secu.lu",
	"description": "Sends mail content to custom indexing programm",
	"default_locale": "en-US",
	"icons": {
		"64": "content/images/ccss_64.png",
		"32": "content/images/ccss_32.png",
		"16": "content/images/ccss_16.png"
	},
	"permissions": ["menus", "messagesRead", "nativeMessaging"],
	"background": {
		"scripts": ["background.js"]
	}
}
